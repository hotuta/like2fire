{
	"name": "like2fire for chrome extension",
	"version": "0.5",
	"manifest_version": 2,
	"description": "change the like button to fire button of Twitter",
	"web_accessible_resources": ["like2fire.css"],
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*","https://tweetdeck.twitter.com/*"],
			"js": ["fire_query.js", "like2fire.js"],
			"css": ["like2fire.css"]
    }
  ],
	"icons": {
		"128": "icon.png"
	}
}